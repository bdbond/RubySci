

<%= form_tag controller:'sessions',action:'create' do %>
<div class="field">
  <%= label_tag :email %>
  <%= text_field_tag :email %>
</div>
<div class="field">
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</div>
<div class="actions">
  <%= submit_tag "Log in" %>
</div>
<% end %>
